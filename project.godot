config_version=5

[application]

config/name="CharacterAnimator"
run/main_scene="res://gui/project_selector/project_selector.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://gui/app_icon.png"

[autoload]

FileSystem="*res://modules/io/file_system.tscn"
ProjectManager="*res://modules/project_manager/project_manager.gd"

[debug]

gdscript/warnings/inference_on_variant=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/default_theme_scale=2.0

[rendering]

renderer/rendering_method="mobile"
